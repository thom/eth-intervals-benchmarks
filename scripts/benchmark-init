echo Logfile: $1
echo

echo "date"
date
echo

cd ../

echo 'git branch'
git branch --no-color 2> /dev/null | sed -e '/^[^*]/d'
echo

echo 'git show --pretty=oneline | head -1'
git show --pretty=oneline | head -1
echo

echo "git diff | cat"
git diff | cat
echo

echo "pwd"
pwd
echo

echo "Settings:"
echo "JAVA_OPTS=${JAVA_OPTS}"
echo "NOBUILD=${NOBUILD}"
echo "CLASSPATH=${CLASSPATH}"
echo

if [ ! -n "$NOBUILD" ]; then
	MYDIR=$(dirname $0)
	DIR=$(cd $MYDIR/../..; pwd)
	cd $DIR
	ant
	echo
fi
